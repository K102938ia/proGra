<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="649.0" prefWidth="906.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.progect3temp.RandomAd">
   <children>
      <Button fx:id="adBack" layoutX="14.0" layoutY="596.0" mnemonicParsing="false" onAction="#goBackRandAd" text="Back">
         <font>
            <Font name="Harlow Solid Italic" size="16.0" />
         </font>
      </Button>
      <ImageView fx:id="adImage" fitHeight="443.0" fitWidth="503.0" layoutX="182.0" layoutY="83.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="adUser" alignment="CENTER" layoutX="266.0" layoutY="28.0" prefHeight="42.0" prefWidth="336.0">
         <font>
            <Font name="Harlow Solid Italic" size="24.0" />
         </font>
      </Label>
      <TextArea fx:id="adText" layoutX="182.0" layoutY="540.0" prefHeight="82.0" prefWidth="503.0">
         <font>
            <Font name="Harlow Solid Italic" size="14.0" />
         </font>
      </TextArea>
      <Button fx:id="moreAds" layoutX="14.0" layoutY="540.0" mnemonicParsing="false" onAction="#moreAds" text="show more ads">
         <font>
            <Font name="Harlow Solid Italic" size="17.0" />
         </font>
      </Button>
      <Label fx:id="noSuitableLabel" alignment="CENTER" layoutX="712.0" layoutY="368.0" prefHeight="38.0" prefWidth="163.0" textFill="#d92323">
         <font>
            <Font name="Harlow Solid Italic" size="18.0" />
         </font>
      </Label>
      <Label fx:id="noAd" layoutX="15.0" layoutY="440.0" prefHeight="38.0" prefWidth="146.0" textFill="#d91c1c">
         <font>
            <Font name="Harlow Solid Italic" size="18.0" />
         </font>
      </Label>
      <ImageView fitHeight="38.0" fitWidth="37.0" layoutX="23.0" layoutY="319.0" onMouseClicked="#like" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@like.png" />
         </image>
      </ImageView>
      <Label fx:id="likeLabel" layoutX="21.0" layoutY="356.0" prefHeight="30.0" prefWidth="134.0">
         <font>
            <Font name="Harlow Solid Italic" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
