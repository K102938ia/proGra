<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="525.0" prefWidth="948.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.progect3temp.StatsController">
   <children>
      <BarChart fx:id="seenChart" layoutY="55.0" prefHeight="422.0" prefWidth="448.0" title="Stats of Seen">
        <xAxis>
          <CategoryAxis label="day" side="BOTTOM" fx:id="xSeen" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="ySeen" label="Seen" side="LEFT" />
        </yAxis>
      </BarChart>
      <BarChart fx:id="likeChart" layoutX="467.0" layoutY="53.0" prefHeight="422.0" prefWidth="467.0" title="Stats of Like">
        <xAxis>
          <CategoryAxis label="day" side="BOTTOM" fx:id="xLike" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yLike" label="Like" side="LEFT" />
        </yAxis>
      </BarChart>
      <Button fx:id="statsBack" layoutX="14.0" layoutY="486.0" mnemonicParsing="false" onAction="#statsGoBack" text="back" />
   </children>
</AnchorPane>
